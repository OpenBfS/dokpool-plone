<tal:block i18n:domain="docpool.base">
  <tal:loop repeat="item python:view.meta_infos()">

    <img class="collapsible"
         tal:condition="python:item['icon_url']"
         tal:attributes="
           src python:item['icon_url'];
         "
    />

    <div tal:content="structure python:item['behavior'].myView('meta', **options)"
         tal:attributes="
           class python: item['behavior_name'] == 'transfers' and 'transfers_content' or 'content';
         "
    >
                Text
    </div>

  </tal:loop>
</tal:block>
