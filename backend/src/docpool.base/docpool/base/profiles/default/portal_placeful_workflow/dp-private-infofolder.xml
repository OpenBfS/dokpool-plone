<?xml version="1.0" encoding="utf-8"?>
<object meta_type="WorkflowPolicy"
        name="dp-private-infofolder"
>
  <property name="title">DP Private Infos Workflow</property>
  <bindings>
    <default>
      <bound-workflow workflow_id="dp_privateinfo_workflow" />
    </default>
    <type type_id="InfoFolder">
      <bound-workflow workflow_id="dp_privateinfo_workflow" />
    </type>
    <type type_id="InfoDocument">
      <bound-workflow workflow_id="dp_privateinfo_workflow" />
    </type>
    <type type_id="InfoLink">
      <bound-workflow workflow_id="dp_privateinfo_workflow" />
    </type>
  </bindings>
</object>
