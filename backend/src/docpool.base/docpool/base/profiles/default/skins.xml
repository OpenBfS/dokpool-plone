<?xml version="1.0"?>

<object name="portal_skins" meta_type="Plone Skins Tool" allow_any="False" cookie_persistence="False" request_varname="plone_skin">

 <object name="docpool_base_images" meta_type="Filesystem Directory View" directory="docpool.base:skins/docpool_base_images"/>
 <object name="docpool_base_templates" meta_type="Filesystem Directory View" directory="docpool.base:skins/docpool_base_templates"/>
 <object name="docpool_base_styles" meta_type="Filesystem Directory View" directory="docpool.base:skins/docpool_base_styles"/>



 <skin-path name="*">
  <layer name="docpool_base_images" insert-after="custom"/>
  <layer name="docpool_base_templates" insert-after="custom"/>
  <layer name="docpool_base_styles" insert-after="custom"/>


 </skin-path>


</object>
