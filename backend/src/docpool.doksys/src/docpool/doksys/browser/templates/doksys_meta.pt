<tal:block define="
             view nocall:options/view;
           "
           i18n:domain="docpool.doksys"
>
  <tal:def define="
             doksysobj view;
           ">
    <!-- context will be the the DPDocument instance, doksysobj will be the behaviour wrapper -->
