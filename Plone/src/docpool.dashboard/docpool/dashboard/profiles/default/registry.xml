<registry xmlns:i18n="http://xml.zope.org/namespaces/i18n"
          i18n:domain="docpool.dashboard">
  <records prefix="plone.resources/docpool-dashboard"
            interface='Products.CMFPlone.interfaces.IResourceRegistry'>
      <value key="js"></value>
      <value key="css">
        <element>++resource++docpool.dashboard.stylesheets/dashboard.less</element>
      </value>
  </records>
  
  <records prefix="plone.bundles/docpool-dashboard"
            interface='Products.CMFPlone.interfaces.IBundleRegistry'>
    <value key="resources">
      <element>docpool-dashboard</element>
    </value>
    <value key="expression"></value>
    <value key="compile">True</value>
    <value key="enabled">True</value>
    <value key="depends">docpool</value>
    <value key="jscompilation"></value>
    <value key="csscompilation">++plone++static/docpool-dashboard-compiled.css</value>    
  </records>


</registry>