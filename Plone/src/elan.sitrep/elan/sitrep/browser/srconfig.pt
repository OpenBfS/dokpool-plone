<html lang="en" xmlns="http://www.w3.org/1999/xhtml" xmlns:tal="http://xml.zope.org/namespaces/tal" i18n:domain="elan.sitrep" xml:lang="en" metal:use-macro="context/main_template/macros/master" xmlns:metal="http://xml.zope.org/namespaces/metal" xmlns:i18n="http://xml.zope.org/namespaces/i18n">
<body>
<!-- ##code-section Head -->
<!-- ##/code-section Head -->    

<metal:content-core fill-slot="content-core">
<metal:content-core define-macro="content-core">
<!-- ##code-section Main -->
<tal:loop repeat="scenario context/getSRScenarios">
<h3><a href="${scenario/absolute_url}">${scenario/Title}</a></h3>
<table class="srscenario listing">
<thead>
<tr>
    <th i18n:translate="">Phase</th>
    <tal:loop repeat="mt scenario/modTypes">
        <th>${python:mt[1]}</th>
    </tal:loop>
</tr>
</thead>
<tbody>
	<tal:loop repeat="phase scenario/getSRPhases">
	<tr><td><a href="${phase/absolute_url}">${phase/Title}</a></td>
	<tal:loop define="mconfigs phase/availableModuleConfigs" repeat="mtid scenario/modTypeIds">
	   <td tal:define="mc python:mconfigs[mtid]"><tal:if condition="nocall:mc">
	   <a href="${mc/absolute_url}">${mc/Title}</a>
	   <ul tal:define="tbs mc/textBlocks" tal:condition="tbs">
	       <tal:loop repeat="tb tbs">
	           <li><a href="${python:tb.to_object.absolute_url()}">${python:tb.to_object.Title()}</a></li>
	       </tal:loop>
	   </ul>
	   </tal:if></td>
	</tal:loop>
	</tr>
	</tal:loop>
</tbody>
</table>

</tal:loop>
<!-- ##/code-section Main -->    
</metal:content-core>
</metal:content-core>

<!-- ##code-section Bottom -->
<!-- ##/code-section Bottom -->  
</body>
</html>
