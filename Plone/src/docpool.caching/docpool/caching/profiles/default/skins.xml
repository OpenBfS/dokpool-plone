<?xml version="1.0"?>

<object name="portal_skins" meta_type="Plone Skins Tool" allow_any="False" cookie_persistence="False" request_varname="plone_skin">

 <object name="docpool_caching_images" meta_type="Filesystem Directory View" directory="docpool.caching:skins/docpool_caching_images"/>
 <object name="docpool_caching_templates" meta_type="Filesystem Directory View" directory="docpool.caching:skins/docpool_caching_templates"/>
 <object name="docpool_caching_styles" meta_type="Filesystem Directory View" directory="docpool.caching:skins/docpool_caching_styles"/>



 <skin-path name="*">
  <layer name="docpool_caching_images" insert-after="custom"/>
  <layer name="docpool_caching_templates" insert-after="custom"/>
  <layer name="docpool_caching_styles" insert-after="custom"/>


 </skin-path>


</object>
