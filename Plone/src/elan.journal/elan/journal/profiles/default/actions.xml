<?xml version="1.0"?>
<object name="portal_actions" meta_type="Plone Actions Tool"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <object name="user" meta_type="CMF Action Category">
  <object name="add_journal1_entry" meta_type="CMF Action" i18n:domain="elan.policy">
   <property name="title" i18n:translate="">Add Journal1 Entry</property>
   <property name="description" i18n:translate=""></property>
   <property
      name="url_expr">string:${context/dp_url}/esd/journal1/update</property>
   <property name="link_target"></property>
   <property name="icon_expr"></property>
   <property name="available_expr">python:here.isAdmin() or here.isContentAdmin() or (member and member.has_role('Journal1 Editor'))</property>
   <property name="permissions"></property>
   <property name="visible">True</property>
  </object>
<object name="add_journal2_entry" meta_type="CMF Action" i18n:domain="elan.policy">
   <property name="title" i18n:translate="">Add Journal2 Entry</property>
   <property name="description" i18n:translate=""></property>
   <property
      name="url_expr">string:${context/dp_url}/esd/journal2/update</property>
   <property name="link_target"></property>
   <property name="icon_expr"></property>
   <property name="available_expr">python:here.isAdmin() or here.isContentAdmin() or (member and member.has_role('Journal2 Editor'))</property>
   <property name="permissions"></property>
   <property name="visible">True</property>
  </object>
 </object>
</object>
