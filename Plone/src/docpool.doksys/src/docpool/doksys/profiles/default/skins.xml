<?xml version="1.0"?>

<object name="portal_skins" meta_type="Plone Skins Tool" allow_any="False" cookie_persistence="False" request_varname="plone_skin">

 <object name="docpool_doksys_images" meta_type="Filesystem Directory View" directory="docpool.doksys:skins/docpool_doksys_images"/>
 <object name="docpool_doksys_templates" meta_type="Filesystem Directory View" directory="docpool.doksys:skins/docpool_doksys_templates"/>
 <object name="docpool_doksys_styles" meta_type="Filesystem Directory View" directory="docpool.doksys:skins/docpool_doksys_styles"/>



 <skin-path name="*">
  <layer name="docpool_doksys_images" insert-after="custom"/>
  <layer name="docpool_doksys_templates" insert-after="custom"/>
  <layer name="docpool_doksys_styles" insert-after="custom"/>


 </skin-path>


</object>
