<registry xmlns:i18n="http://xml.zope.org/namespaces/i18n"
          i18n:domain="docpool.theme">
  <records prefix="plone.resources/docpool-menu"
            interface='Products.CMFPlone.interfaces.IResourceRegistry'>
      <value key="js">++plone++docpool.menu/init-menu.js</value>
      <value key="css">
        <element>++plone++docpool.menu/responsive-menu.less</element>
      </value>
      <value key="deps">docpool-menu-plugin</value>
  </records>
  <records prefix="plone.resources/docpool-menu-plugin"
            interface='Products.CMFPlone.interfaces.IResourceRegistry'>
      <value key="js">++plone++docpool.menu/responsive-menu.js</value>
      <value key="deps">jquery</value>
      <value key="export"></value>
  </records>
</registry>
