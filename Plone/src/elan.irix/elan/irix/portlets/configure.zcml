            <plone:portlet interface=".irix_navigation.Iirix_navigationPortlet" assignment=".irix_navigation.Assignment" addview=".irix_navigation.AddForm" name="elan.irix.irix_navigation" renderer=".irix_navigation.Renderer" />




</configure>