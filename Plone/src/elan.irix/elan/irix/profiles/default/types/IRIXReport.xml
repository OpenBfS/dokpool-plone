<?xml version="1.0"?>
<object meta_type="Dexterity FTI" xmlns:i18n="http://xml.zope.org/namespaces/i18n" i18n:domain="elan.irix" name="IRIXReport">

 <!-- Basic metadata -->
 <property name="title" i18n:translate="">IRIX Report</property>
 <property name="description" i18n:translate="">Represents a single IRIX report whose data is stored in a relational database.</property>
 <property name="content_icon"></property>
 <property name="allow_discussion">False</property>
 <property name="global_allow">True</property>
 <property name="filter_content_types">True</property>
 <property name="allowed_content_types">

 </property>
 <!-- schema interface -->
 <property name="schema">elan.irix.content.irixreport.IIRIXReport</property>

 <!-- class used for content items -->
 <property name="klass">elan.irix.content.irixreport.IRIXReport</property>

 <!-- add permission -->
 <property name="add_permission">elan.irix.AddIRIXReport</property>

 <!-- enabled behaviors -->
 <property name="behaviors">
     <element value="plone.app.dexterity.behaviors.metadata.IDublinCore" />
     <element value="plone.app.content.interfaces.INameFromTitle" />
     <element value="collective.dexteritytextindexer.behavior.IDexterityTextIndexer" />
     <element value="plone.app.dexterity.behaviors.discussion.IAllowDiscussion" />
     <element value="plone.app.dexterity.behaviors.exclfromnav.IExcludeFromNavigation" />
</property>

 <!-- View information -->
 <property name="default_view">struct_edit</property>
 <property name="default_view_fallback">False</property>
 <property name="view_methods">
  <element value="view" />
  <element value="base_view" />

 </property>
 <alias from="(Default)" to="(dynamic view)" />
 <alias from="edit" to="@@edit" />
 <alias from="sharing" to="@@sharing" />
 <alias from="view" to="(selected layout)" />

 <!-- Actions -->
 <action title="View" action_id="view" category="object" condition_expr="" url_expr="string:${object_url}" visible="True">
  <permission value="View" />
 </action>
 <action title="Edit" action_id="edit" category="object" condition_expr="" url_expr="string:${object_url}/edit" visible="True">
  <permission value="Modify portal content" />
 </action>


</object>