<?xml version="1.0"?>

<object name="portal_skins" meta_type="Plone Skins Tool" allow_any="False" cookie_persistence="False" request_varname="plone_skin">

 <object name="elan_esd_images" meta_type="Filesystem Directory View" directory="elan.esd:skins/elan_esd_images"/>
 <object name="elan_esd_templates" meta_type="Filesystem Directory View" directory="elan.esd:skins/elan_esd_templates"/>
 <object name="elan_esd_styles" meta_type="Filesystem Directory View" directory="elan.esd:skins/elan_esd_styles"/>



 <skin-path name="*">
  <layer name="elan_esd_images" insert-after="custom"/>
  <layer name="elan_esd_templates" insert-after="custom"/>
  <layer name="elan_esd_styles" insert-after="custom"/>


 </skin-path>


</object>
