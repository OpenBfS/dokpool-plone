<tal:block i18n:domain="elan.esd">
<tal:if condition="not:here/isArchive">
<div metal:use-macro="here/@@elan.macros/macros/scenario_table"/>
</tal:if>
<tal:if condition="here/isArchive">
<tal:def define="a_obj here/myELANArchive|nothing" condition="nocall: a_obj">
<div class="arch_scen">
<h1><i18n:var translate=""> Archive</i18n:var> <tal:var replace="a_obj/Title"/></h1>
<p tal:content="a_obj/Description"></p>
<p><br><b>&rarr; <a href="" tal:attributes="href python:context.myELANArchives().absolute_url()" i18n:translate="">All Archives</a></b></p>
</div>
</tal:def>
</tal:if>
</tal:block>