 <registry xmlns:i18n="http://xml.zope.org/namespaces/i18n"
          i18n:domain="docpool.theme">
  <records prefix="plone.resources/docpool"
            interface='Products.CMFPlone.interfaces.IResourceRegistry'>
      <value key="js">++theme++docpool.theme/static/docpool.js</value>
      <value key="css">
        <element>++theme++docpool.theme/static/docpool.less</element>
      </value>
  </records>
  <records prefix="plone.resources/docpool-anon"
            interface='Products.CMFPlone.interfaces.IResourceRegistry'>
      <value key="css">
        <element>++theme++docpool.theme/static/anonymous.less</element>
      </value>
  </records>
  <records prefix="plone.resources/docpool-nonadmin"
            interface='Products.CMFPlone.interfaces.IResourceRegistry'>
      <value key="css">
        <element>++theme++docpool.theme/static/simplify.less</element>
      </value>
  </records>
  <records prefix="plone.resources/jquery-marquee"
            interface='Products.CMFPlone.interfaces.IResourceRegistry'>
      <value key="js">++theme++docpool.theme/static/jquery.marquee.js</value>
  </records>
  <records prefix="plone.resources/docpool-functions"
            interface='Products.CMFPlone.interfaces.IResourceRegistry'>
      <value key="js">++theme++docpool.theme/static/functions.js</value>
  </records>
  <records prefix="plone.resources/intercom"
            interface='Products.CMFPlone.interfaces.IResourceRegistry'>
      <value key="js">++theme++docpool.theme/static/intercom.min.js</value>
  </records>
  <records prefix="plone.resources/notify"
            interface='Products.CMFPlone.interfaces.IResourceRegistry'>
      <value key="js">++theme++docpool.theme/static/notify.min.js</value>
  </records>
  <records prefix="plone.resources/domReady"
            interface='Products.CMFPlone.interfaces.IResourceRegistry'>
      <value key="js">++theme++docpool.theme/static/domReady.js</value>
  </records>
  <records prefix="plone.bundles/docpool"
            interface='Products.CMFPlone.interfaces.IBundleRegistry'>
    <value key="resources">
      <element>docpool</element>
    </value>
    <value key="expression"></value>
    <value key="compile">True</value>
    <value key="enabled">True</value>
    <value key="depends">plone</value>
    <value key="jscompilation">++plone++static/docpool-compiled.js</value>
    <value key="csscompilation">++plone++static/docpool-compiled.css</value>    
  </records>
  <records prefix="plone.bundles/docpool-anon"
            interface='Products.CMFPlone.interfaces.IBundleRegistry'>
    <value key="resources">
      <element>docpool-anon</element>
    </value>
    <value key="expression">python: member is None</value>
    <value key="compile">True</value>
    <value key="enabled">True</value>
    <value key="jscompilation"></value>
    <value key="csscompilation">++plone++static/docpool-anon-compiled.css</value>    
  </records>
  <records prefix="plone.bundles/docpool-nonadmin"
            interface='Products.CMFPlone.interfaces.IBundleRegistry'>
    <value key="resources">
      <element>docpool-nonadmin</element>
    </value>
    <value key="expression">python:member and not (member.has_role('Manager') or member.has_role('Site Administrator')) or not object.isContentAdmin() or None</value>
    <value key="compile">True</value>
    <value key="enabled">True</value>
    <value key="jscompilation"></value>
    <value key="csscompilation">++plone++static/docpool-nonadmin-compiled.css</value>    
  </records>
</registry>