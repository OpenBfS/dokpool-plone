<div id="portal-logo">
<a metal:define-macro="portal_logo"
   id="portal-logo-img"
   title="Home"
   accesskey="1"
   tal:attributes="href string:${view/portal_url};
                   title string:DocPool"
   i18n:domain="elan.esd"
   i18n:attributes="title">
        <img src="logo.png" alt=""
             tal:define="logo context/logoSrc|nothing;
                         logo python:logo or view.img_src;"
         tal:attributes="alt view/logo_title;
                         title view/logo_title;
                         src logo" /></a><p class="version" tal:content="context/docpoolversion|nothing">1.0</p>
   
</div>
    <tal:var replace="structure context/logoutjs|nothing" />
