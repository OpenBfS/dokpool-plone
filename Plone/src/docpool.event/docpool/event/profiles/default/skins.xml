<?xml version="1.0"?>

<object name="portal_skins" meta_type="Plone Skins Tool" allow_any="False" cookie_persistence="False" request_varname="plone_skin">

 <object name="docpool_event_images" meta_type="Filesystem Directory View" directory="docpool.event:skins/docpool_event_images"/>
 <object name="docpool_event_templates" meta_type="Filesystem Directory View" directory="docpool.event:skins/docpool_event_templates"/>
 <object name="docpool_event_styles" meta_type="Filesystem Directory View" directory="docpool.event:skins/docpool_event_styles"/>



 <skin-path name="*">
  <layer name="docpool_event_images" insert-after="custom"/>
  <layer name="docpool_event_templates" insert-after="custom"/>
  <layer name="docpool_event_styles" insert-after="custom"/>


 </skin-path>


</object>
