<?xml version="1.0"?>
<object name="portal_actions" meta_type="Plone Actions Tool"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <action-provider name="portal_actions"/>
 <object name="document_actions" meta_type="CMF Action Category">
  <property name="title"></property>
  <object name="rss" meta_type="CMF Action" i18n:domain="plone">
   <property name="visible">True</property>
  </object>
  <object name="print" meta_type="CMF Action" i18n:domain="plone">
   <property name="visible">True</property>
  </object>
  <object name="irix" meta_type="CMF Action" i18n:domain="elan.policy">
   <property name="title" i18n:translate="">IRIX Export</property>
   <property name="description" i18n:translate=""></property>
   <property
      name="url_expr">string:${object/absolute_url}/@@irix</property>
   <property name="link_target"></property>
   <property name="icon_expr"></property>
   <property name="available_expr">here/withIRIXExport</property>
   <property name="permissions">
    <element value="Manage portal"/>
   </property>
   <property name="visible">True</property>
  </object>
  <object name="full_screen" meta_type="CMF Action" i18n:domain="plone">
   <property name="visible">False</property>
  </object>
  <object name="extedit" meta_type="CMF Action" i18n:domain="plone">
   <property name="visible">False</property>
  </object>
 </object>
 <object name="site_actions" meta_type="CMF Action Category">
  <property name="title"></property>
  <object name="sitemap" meta_type="CMF Action" i18n:domain="plone">
   <property name="visible">False</property>
  </object>
  <object name="accessibility" meta_type="CMF Action" i18n:domain="plone">
   <property name="visible">False</property>
  </object>
  <object name="contact" meta_type="CMF Action" i18n:domain="plone">
   <property name="visible">False</property>
  </object>
  <object name="impressum" meta_type="CMF Action" i18n:domain="elan.policy">
   <property name="title" i18n:translate="">Impressum</property>
   <property name="description" i18n:translate=""></property>
   <property
      name="url_expr">string:${context/dp_url}/contentconfig/impressum</property>
   <property name="link_target"></property>
   <property name="icon_expr"></property>
   <property name="available_expr"></property>
   <property name="permissions">
    <element value="View"/>
   </property>
   <property name="visible">True</property>
  </object>
  <object name="help" meta_type="CMF Action" i18n:domain="elan.policy">
   <property name="title" i18n:translate="">Help</property>
   <property name="description" i18n:translate=""></property>
   <property
      name="url_expr">string:${context/dp_url}/@@help</property>
   <property name="link_target"></property>
   <property name="icon_expr"></property>
   <property name="available_expr"></property>
   <property name="permissions">
    <element value="View"/>
   </property>
   <property name="visible">True</property>
  </object>
 </object>
 <object name="portal_tabs" meta_type="CMF Action Category">
  <property name="title"></property>
  <object name="index_html" meta_type="CMF Action" i18n:domain="plone">
   <property name="visible">False</property>
  </object>
 </object> 
 <object name="user" meta_type="CMF Action Category">
  <property name="title"></property>
  <object name="mystuff" meta_type="CMF Action" i18n:domain="plone">
   <property name="visible">False</property>
  </object>
  <object name="dashboard" meta_type="CMF Action" i18n:domain="plone">
   <property name="visible">False</property>
  </object>
  <object name="preferences" meta_type="CMF Action" i18n:domain="plone">
   <property name="title" i18n:translate="">Change Password</property>
   <property
      name="url_expr">string:${context/dp_url}/@@change-password</property>
   <property name="available_expr">here/canChangePassword</property>
   <property name="visible">True</property>
  </object>
  <object name="applications" meta_type="CMF Action" i18n:domain="elan.policy">
   <property name="title" i18n:translate="">Select Applications</property>
   <property
      name="url_expr">string:${context/dp_url}/@@personal-preferences</property>
   <property name="visible">True</property>
  </object>
  <object name="review-comments" meta_type="CMF Action" i18n:domain="plone">
   <property name="visible">False</property>
  </object>
  <object name="plone_setup" meta_type="CMF Action" i18n:domain="elan.policy">
   <property name="title" i18n:translate="">User Management</property>
   <property name="description" i18n:translate=""></property>
   <property
      name="url_expr">string:${context/dp_url}/@@usergroup-userprefs</property>
   <property name="icon_expr"></property>
   <property name="available_expr">here/isAdmin</property>
   <property name="permissions"></property>
   <property name="visible">True</property>
  </object>
  <object name="edit_scenarios" meta_type="CMF Action" i18n:domain="elan.policy">
   <property name="title" i18n:translate="">Edit Scenarios</property>
   <property name="description" i18n:translate=""></property>
   <property
      name="url_expr">string:${context/dp_url}/contentconfig/scen/</property>
   <property name="link_target"></property>
   <property name="icon_expr"></property>
   <property name="available_expr">here/isContentAdmin</property>
   <property name="permissions"></property>
   <property name="visible">True</property>
  </object>
  <object name="edit_scenarios" meta_type="CMF Action" i18n:domain="elan.policy">
   <property name="title" i18n:translate="">Add New Scenario</property>
   <property name="description" i18n:translate=""></property>
   <property
      name="url_expr">string:${context/dp_url}/contentconfig/scen/++add++ELANScenario</property>
   <property name="link_target"></property>
   <property name="icon_expr"></property>
   <property name="available_expr">here/isContentAdmin</property>
   <property name="permissions"></property>
   <property name="visible">True</property>
  </object>
  <object name="edit_dashboard" meta_type="CMF Action" i18n:domain="elan.policy">
   <property name="title" i18n:translate="">Edit Dashboard</property>
   <property name="description" i18n:translate=""></property>
   <property
      name="url_expr">string:${context/dp_url}/esd/dashboard/edit</property>
   <property name="link_target"></property>
   <property name="icon_expr"></property>
   <property name="available_expr">here/isContentAdmin</property>
   <property name="permissions"></property>
   <property name="visible">True</property>
  </object>
  <object name="edit_ticker" meta_type="CMF Action" i18n:domain="elan.policy">
   <property name="title" i18n:translate="">Edit Ticker</property>
   <property name="description" i18n:translate=""></property>
   <property
      name="url_expr">string:${context/dp_url}/contentconfig/ticker/edit</property>
   <property name="link_target"></property>
   <property name="icon_expr"></property>
   <property name="available_expr">here/isContentAdmin</property>
   <property name="permissions"></property>
   <property name="visible">True</property>
  </object>
  <object name="delete_ticker" meta_type="CMF Action" i18n:domain="elan.policy">
   <property name="title" i18n:translate="">Delete Ticker</property>
   <property name="description" i18n:translate=""></property>
   <property
      name="url_expr">string:${context/dp_url}/contentconfig/ticker/deleteTicker</property>
   <property name="link_target"></property>
   <property name="icon_expr"></property>
   <property name="available_expr">here/isContentAdmin</property>
   <property name="permissions"></property>
   <property name="visible">True</property>
  </object>
  <object name="logout" meta_type="CMF Action" i18n:domain="plone">
   <property name="title" i18n:translate="">Log out</property>
   <property name="description" i18n:translate=""></property>
   <property
      name="url_expr">string:javascript:close_overview()</property>
   <property name="link_target"></property>
   <property name="icon_expr"></property>
   <property name="available_expr">python:member is not None</property>
   <property name="permissions">
    <element value="View"/>
   </property>
   <property name="visible">True</property>
  </object>
 </object>
 <object name="object_buttons" meta_type="CMF Action Category">
  <object name="ical_import_enable" meta_type="CMF Action" i18n:domain="plone.app.event">
   <property name="visible">False</property>
  </object>
  <object name="ical_import_disable" meta_type="CMF Action" i18n:domain="plone.app.event">
   <property name="visible">False</property>
  </object>
  <property name="title"></property>
  <object name="copy" meta_type="CMF Action" i18n:domain="plone">
   <property name="visible">False</property>
  </object>
  <object name="rename" meta_type="CMF Action" i18n:domain="plone">
   <property name="visible">False</property>
  </object>
  <object name="snapshot" meta_type="CMF Action" i18n:domain="elan.esd">
   <property name="title" i18n:translate="">Snapshot</property>
   <property name="description" i18n:translate=""></property>
   <property name="url_expr">string:${object/absolute_url}/snapshot</property>
   <property name="link_target"></property>
   <property name="icon_expr"></property>
   <property
      name="available_expr">python:object.getPortalTypeName() == 'ELANScenario' and object.status != 'closed' and (here.isAdmin() or here.isContentAdmin())</property>
   <property name="permissions" />
   <property name="visible">True</property>
  </object>
  <object name="purge" meta_type="CMF Action" i18n:domain="elan.esd">
   <property name="title" i18n:translate="">Purge</property>
   <property name="description" i18n:translate=""></property>
   <property name="url_expr">string:javascript: if (confirm('${object/purgeConfirmMsg}')) window.location.href='${object/absolute_url}/purge'</property>
   <property name="link_target"></property>
   <property name="icon_expr"></property>
   <property
      name="available_expr">python:object.getPortalTypeName() == 'ELANScenario' and object.status == 'inactive' and (here.isAdmin() or here.isContentAdmin())</property>
   <property name="permissions" />
   <property name="visible">True</property>
  </object>
  <object name="archive" meta_type="CMF Action" i18n:domain="elan.esd">
   <property name="title" i18n:translate="">Archiving</property>
   <property name="description" i18n:translate=""></property>
   <property
      name="url_expr">string:javascript: if (confirm('${object/archiveConfirmMsg}')) window.location.href='${object/absolute_url}/archiveAndClose'</property>
   <property name="link_target"></property>
   <property name="icon_expr"></property>
   <property
      name="available_expr">python:object.getPortalTypeName() == 'ELANScenario' and object.status == 'inactive' and (here.isAdmin() or here.isContentAdmin())</property>
   <property name="permissions" />
   <property name="visible">True</property>
  </object>
 </object>
 <object name="folder_buttons" meta_type="CMF Action Category">
  <property name="title"></property>
  <object name="delete" meta_type="CMF Action" i18n:domain="plone">
   <property name="permissions">
    <element value="View management screens"/>
   </property>
  </object> 
  <object name="change_state" meta_type="CMF Action" i18n:domain="plone">
    <property name="visible">False</property>
  </object> 
 </object> 
</object>
